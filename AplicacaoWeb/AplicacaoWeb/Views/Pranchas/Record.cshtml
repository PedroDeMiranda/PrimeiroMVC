@using AplicacaoWeb.Models.Pranchas
@model PranchaModel


<div>
    <h1>Prancha</h1>
    <form>
        <div class="form-group">
            <label>Modelo</label>
            <input type="text" class="form-label" id="modelo" asp-for="Modelo">
        </div>

        <div class="form-group">
            <label>Litragem</label>
            <input type="text" class="form-label" id="placa" asp-for="Litragem">
        </div>

        <div class="form-group">
            <label>Altura</label>
            <input type="number" class="form-label" id="ano" asp-for="Altura">
        </div>

    </form>

    <button type="submit" class="btn btn-primary">Editar Carro</button>
</div>
